<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--<start id="duke_bean" />-->
    <bean id="duke" class="com.springinaction.springidol.Juggler">
        <constructor-arg value="15"/>
    </bean>
    <bean class="com.springinaction.springidol.PoeticJuggler" id="poeticJuggler1">
        <constructor-arg ref="sonnet29"/>
    </bean>
    <bean class="com.springinaction.springidol.PoeticJuggler" id="poeticJuggler2">
        <constructor-arg value="15"/>
        <constructor-arg ref="sonnet29"/>
    </bean>
    <bean class="com.springinaction.springidol.Sonnet29" id="sonnet29"/>
    <bean class="com.springinaction.springidol.PoeticJuggler" id="poeticJuggler">
        <constructor-arg value="15"/>
        <constructor-arg>
            <bean class="com.springinaction.springidol.Sonnet29"/>
        </constructor-arg>
    </bean>
    <!--<end id="duke_bean" />-->

    <bean class="com.springinaction.springidol.Stage" id="theStage" factory-method="getInstance"/>
    <bean class="com.springinaction.springidol.Instrumentalist" id="kenny1">
        <property name="song" value="jingle bells"/>
        <property name="age" value="37"/>
    </bean>
    <bean class="com.springinaction.springidol.Saxophone" id="saxophone"/>

    <bean class="com.springinaction.springidol.Instrumentalist" id="kenny2">
        <property name="song" value="jingle bells"/>
        <property name="age" value="37"/>
        <property name="instrument" ref="saxophone"/>
    </bean>
    <!-- Inner Bean-->
    <bean class="com.springinaction.springidol.Instrumentalist" id="kenny">
        <property name="song" value="jingle bells"/>
        <property name="age" value="37"/>
        <property name="instrument"><!-- Inner Bean-->
            <bean class="com.springinaction.springidol.Saxophone"/>
        </property>
    </bean>
    <bean class="com.springinaction.springidol.Piano" id="piano"/>
</beans>


















